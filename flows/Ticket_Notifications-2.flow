    <decisions>
        <name>Message_Sent</name>
        <label>Message Sent?</label>
        <locationX>221</locationX>
        <locationY>286</locationY>
        <defaultConnector>
            <targetReference>Campaign_Info</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Not Sent</defaultConnectorLabel>
        <rules>
            <name>Sent</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>MessageSent</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <label>Sent</label>
        </rules>
    </decisions>
    <formulas>
        <name>RegistrantCityStateZip</name>
        <dataType>String</dataType>
        <expression>{!RegistrantCity} + &apos;, &apos; + {!RegistrantState} + &apos;  &apos; + {!RegistrantZip}</expression>
    </formulas>
